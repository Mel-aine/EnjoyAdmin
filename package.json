{
  "name": "enjoy-admin",
  "version": "2.0.1",
  "private": true,
  "type": "module",
  "main": "electron/main.js",
  "homepage": "./",
  "scripts": {
    "start": "vite preview --host --port $PORT",
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && cross-env NODE_ENV=development electron electron/main.js\"",
    "electron:build": "cross-env ELECTRON=true npm run build && electron-builder",
    "electron:pack": "cross-env ELECTRON=true npm run build && electron-builder --dir",
    "electron:dist": "cross-env ELECTRON=true npm run build && electron-builder --publish=never"
  },
  "dependencies": {
    "@cloudinary/url-gen": "^1.21.0",
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/list": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@fullcalendar/vue3": "^6.1.15",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@vueuse/core": "^13.1.0",
    "axios": "^1.8.4",
    "bootstrap-icons": "^1.13.1",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "flatpickr": "^4.6.13",
    "html2pdf.js": "^0.10.3",
    "jsvectormap": "^1.6.0",
    "lodash": "^4.17.21",
    "lucide-vue-next": "^0.474.0",
    "luxon": "^3.6.1",
    "papaparse": "^5.5.3",
    "pdfjs-dist": "^5.4.149",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "swiper": "^11.2.1",
    "tippy.js": "^6.3.7",
    "uuid": "^11.1.0",
    "vue": "^3.5.13",
    "vue-flatpickr-component": "^11.0.5",
    "vue-i18n": "^11.1.3",
    "vue-router": "^4.5.0",
    "vue-to-print": "^1.4.0",
    "vue-toastification": "^2.0.0-rc.5",
    "vue3-pdf-app": "^1.0.3",
    "vuedraggable": "^4.1.0",
    "vuevectormap": "^2.0.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/google.maps": "^3.58.1",
    "@types/lodash": "^4.17.17",
    "@types/node": "^22.10.7",
    "@types/papaparse": "^5.3.16",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.3.0",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "electron": "^37.3.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.18.0",
    "eslint-plugin-vue": "^9.32.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "rollup-plugin-visualizer": "^6.0.0",
    "sass-embedded": "^1.83.4",
    "tailwindcss": "^4.0.0",
    "typescript": "~5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-vue-devtools": "^7.7.0",
    "vue-tsc": "^2.2.0",
    "wait-on": "^8.0.4"
  },
  "build": {
    "appId": "com.enjoyadmin.app",
    "productName": "Enjoy Admin",
    "directories": {
      "output": "dist-electron",
      "buildResources": "E:\\temp\\electron-build"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "public/**/*",
      "electron/**/*",
      "package.json"
    ],
    "extraMetadata": {
      "main": "electron/main.js"
    },
    "icon": "src/assets/images/header/logo2.png",
    "asar": false,
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "icon": "public/favicon.ico"
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "public/favicon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  }
}
